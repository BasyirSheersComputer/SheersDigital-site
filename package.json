    "test-all-forms": "node scripts/test-all-forms-comprehensive.js",
    "debug-rls": "node scripts/debug-rls-issue.js",
    "test-without-rls": "node scripts/test-without-rls.js",
    "final-diagnostic": "node scripts/final-diagnostic.js",
    "test-admin": "node scripts/admin-database-setup.js",
    "fix-rls-admin": "node scripts/fix-rls-with-admin.js",
    "test-admin-forms": "node scripts/disable-rls-admin.js",
    "complete-fix": "node scripts/complete-database-fix.js",
    "disable-rls": "node scripts/disable-rls-directly.js",
    "final-solution": "node scripts/final-solution.js",
