  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint .",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "test-keywords": "node scripts/test-keyword-rankings.js",
